{
  "name": "next-chrome-ext",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:api": "next dev -p 3000",
    "build": "next build && npm run prepare",
    "build:dev": "NEXT_PUBLIC_API_MODE=development next build && npm run prepare",
    "build:prod": "NEXT_PUBLIC_API_MODE=production next build && npm run prepare",
    "prepare": "mv out/_next out/assets && sed -i '' 's/\\/_next/\\/assets/g' out/**/**.html && npm run extract-inline-scripts",
    "extract-inline-scripts": "node scripts/extract-inline-scripts.js",
    "start": "next start",
    "lint": "biome lint .",
    "lint:fix": "biome check --apply .",
    "format": "biome format .",
    "format:fix": "biome format --write ."
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^1.1.18",
    "@ai-sdk/openai": "^1.3.1",
    "@ai-sdk/react": "^1.2.0",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "ai": "^4.2.6",
    "next": "^15.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "tailwind-merge": "^3.0.2"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tailwindcss/postcss": "^4.0.17",
    "@types/node": "^20.17.28",
    "@types/react": "^19.0.12",
    "@types/react-dom": "^19.0.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.485.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.17",
    "tw-animate-css": "^1.2.5",
    "typescript": "^5"
  }
}
